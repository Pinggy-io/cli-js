{
  "name": "pinggy",
  "version": "0.1.10",
  "license": "Apache-2.0",
  "type": "module",
  "description": "Create secure, shareable tunnels to your localhost and manage them from the command line. ",
  "main": ".dist/index.cjs",
  "module": ".dist/index.js",
  "types": ".dist/index.d.ts",
  "bin": {
    "pinggy": "dist/index.js"
  },
  "scripts": {
    "build:tsc": "tsc",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "jest --config jest.config.js",
    "dev": "npm link @pinggy/pinggy && npm run build && npm link"
  },
    "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },

  "dependencies": {
    "@pinggy/pinggy": "^0.2.12",
    "chalk": "^5.6.2",
    "clipboardy": "^5.0.0",
    "fullscreen-ink": "^0.1.0",
    "ink": "^6.3.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "mime": "^4.1.0",
    "ora": "^9.0.0",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.1.1",
    "uuid": "^12.0.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^19.2.0",
    "@types/ws": "^8.18.1",
    "jest": "^30.1.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pinggy-io/cli-js.git"
  }
}
